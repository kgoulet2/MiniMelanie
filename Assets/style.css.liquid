/* Testimonial Carousel */

.testimonials-carousel-wrapper {
    position: relative;
    width: 100%;
    max-width: 100%;
    background-color: #f3957e;
     opacity: 0.9;
    background-image: url(https://cdn.shopify.com/s/files/1/1418/7404/t/3/assets/splatter-seamless.jpg?3545910216210061752);
    background-repeat: repeat;
    background-size: 300px;
    margin: 2em auto;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
    padding: 20px 40px 70px;
    z-index: 1;
  overflow: hidden;
}

.testimonials-carousel-wrapper::after {
    content: close-quote;
    position: absolute;
    top: 0px;
    right: 0;
    width: 50px;
    height: 50px;
    padding-top: 8px;
    margin-left: 0px;
    background: #000;
    font-family: "Georgia", serif;
    color: #f3957e;
    font-size: 70px;
    font-size: 4.375rem;
    text-align: center;
    display: none;
}

.testimonials-carousel-wrapper::before {
    content: open-quote;
    position: absolute;
    top: 0px;
    left: 0;
    width: 50px;
    height: 50px;
    padding-top: 8px;
    margin-left: 0px;
    background: #000;
    font-family: "Georgia", serif;
    color: #f3957e;
    font-size: 70px;
    font-size: 4.375rem;
    text-align: center;
    display: none;
  	z-index: 10;
}

@media only screen and (min-width: 768px) {
  .testimonials-carousel-wrapper {
      margin: 4em auto;
      padding: 80px 15% 30px;
  }
}
@media only screen and (min-width: 1170px) {
  .testimonials-carousel-wrapper::after {
    /* quotation mark visible */
    display: block;
  }
  
  .testimonials-carousel-wrapper::before {
    /* quotation mark visible */
    display: block;
  }
}

