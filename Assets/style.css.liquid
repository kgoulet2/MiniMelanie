/*****************************************************************************
 * 01. Base Content Styles
 */
html body * {
  	-webkit-font-smoothing: antialiased;
}

body {
	background-color: {{ settings.bg_color }};
	font-family: {% if settings.general_font != '' %}"{{settings.general_font | replace: '+', ' '}}", {% endif %} {{ settings.general_font_backup }};
	font-size: {{settings.general_font_size}}px;
	color: {{ settings.text_color }};
	{% if settings.body_bg_img %}
	  {% case settings.body_bg_alignment %}
	    {% when 'center' %}
	    background: url({{ 'background-user-uploaded.jpg' | asset_url }}) center center no-repeat {{ settings.bg_color }};
	    {% when 'cover' %}
	    background: url({{ 'background-user-uploaded.jpg' | asset_url }}) center center no-repeat {{ settings.bg_color }};
  		background-size: cover;
	    {% when 'tile' %}
	    background: url({{ 'background-user-uploaded.jpg' | asset_url }}) {{ settings.bg_color }};
	  {% endcase %}
	{% endif %}
}

h1,
h2,
h3,
h4,
h5,
h6 {
	color: {{ settings.header_color }};
	line-height: 1.2;
	font-weight: bold;
	margin: 0 0 .5em 0;
}

h1.theme,
h2.theme,
h3.theme,
h4.theme,
h5.theme,
h6.theme {
  	text-align: center;
	font-size: {{ settings.heading_font_size }}px;
	font-family:{% if settings.header_font != '' %}"{{settings.header_font | replace: '+', ' ' }}",{% endif %} {{ settings.header_font_backup }};
    text-transform: {{ settings.heading_text_transform }};
  	position: relative;
  	z-index: 11;
}
h4.theme,
h5.theme,
h6.theme {
    color: {{ settings.text_color }};
  	font-weight: normal;
	font-size: {{ settings.heading_font_size | minus: 6 }}px;
}
h1 {
	font-size: 48px;
}
h2 {
	font-size: 36px;
}
h3 {
	font-size: 24px;
}
h4 {
	font-size: 18px;
}
h5 {
	font-size: 16px;
}
h6 {
	font-size: 14px;
}
p {
	color: {{ settings.text_color }};
	line-height: 1.7;
	font-weight: normal;
	margin: 0 0 0.5em;
	font-size:{{settings.general_font_size}}px;
}
a {
    text-decoration: underline;
	color: {{ settings.link_color }}
}
a:hover {
	color: {{ settings.link_hover_color }}
}
blockquote {
	padding-left: 1.5em;
	border-left: 5px solid {{ settings.link_color }};
	margin: 0 0 1.5em 1.25em;
}
ul, ol{
	margin: 0 0 1.5em 1.3em;
}
li{
	margin: 0 0 0.25em 0;
}
.hr {
	width: 100%;
	height: 1px;
	background: transparent;
	border-bottom: 1px solid {{settings.site_border_color}};
	margin: 19px 0 20px 0;
}
.mobile {
 	display: none;
}
.mobile-small {
	display: none;
}

/* table and DL styles */

table {
    border-collapse: collapse;
    border-spacing: 0;
    border: 1px solid {{ settings.site_border_color }};
    margin: 1em 0;
}
table tr > th,
table tr > td {
    border-bottom: 1px solid {{ settings.site_border_color }};
    border-right: 1px solid {{ settings.site_border_color }};
    padding: 10px;
    line-height: 1.3;
    vertical-align: middle;
}
table tr > th {
	color: {{ settings.header_color }};
	text-transform:{{settings.heading_text_transform}};
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
}
.table {
 	display: table;
  	width: 100%;
  	height: 100%;
}
.table-cell {
 	display: table-cell;
  	width: 100%;
  	height: 100%;
  	vertical-align: middle;
}
dl {
  margin-bottom: 1.5em;
}
dt,
dd {
  line-height: 1.5em;
}
dt {
  font-weight: bold;
}
dd {
  margin-left: .8em;
}

/* form element styles */

input[type="text"],
input[type="email"],
input[type="password"],
input[type="tel"],
textarea {
  	height: auto;
  	width: 100%;
 	max-width: 768px;
	border: solid 1px #bbbbbb;
  	-moz-border-radius: 0;
  	border-radius: 3px;
	background: #fff;
	color: {{ settings.input_text_color }};
	padding: 5px 14px;
	font-family: {% if settings.general_font != '' %}"{{settings.general_font | replace: '+', ' '}}", {% endif %} {{ settings.general_font_backup }};
	font-size: {{settings.general_font_size}}px;
  	-webkit-appearance: none;
}
textarea {
	resize: vertical;
  	height: 160px;
  
}
input[type="image"]{
	border: none;
}
button {
	width: auto;
	overflow: visible;
}
.btn,
.button,
button {
  	border: 0;
  	-moz-border-radius: 0;
  	border-radius: 0;
  	display: inline-block;
	background:{{ settings.btn_color }};
    font-weight: {{ settings.secondary_font_style }};
	margin: 0 0 .5em 0;
	color:{{ settings.btn_text_color }};
	padding:10px 20px;
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
	-webkit-transition: background 0.25s ease-in;
	-moz-transition: background 0.25s ease-in;
	-ms-transition: background 0.25s ease-in;
	-o-transition: background 0.25s ease-in;
	transition: background 0.25s ease-in;
    font-size: {{ settings.secondary_font_size }}px;
  	-webkit-appearance: none;
}
.btn:hover,
.button:hover,
button:hover {
	background:{{ settings.btn_hover_color }};
	color:{{ settings.btn_hover_text_color }};
	cursor:pointer;
}
.btn.sml,
.buttom.sml {
    font-size: 0.9em;
    padding: 15px;
}
input.error{
	color: #990000;
	color:rgb(193, 87, 82);
}
label {
	vertical-align:top;
	display:inline-block;
  	padding: 5px 0;
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
}
input[type="checkbox"], input.checkbox, input[type="radio"], input.radio {
	margin-top:3px;
	margin-right:10px;
	float:left;
}
.form-row label {
	vertical-align:middle;
	display:inline-block;
}
input:focus {
	outline:none;
}
select {
	margin-left:0;
    outline: none !important;
 	border: 1px solid #bbb;
    border-radius: 3px;
    max-width: 768px !important;
  	width: 100%;
    padding: 5px 14px;
    line-height: 1;
    height: 50px;
    -webkit-appearance: none;
  	-webkit-padding-end: 30px;
  	background: url({{ 'icons-carat.png' | asset_url }}) right center no-repeat #fff;
  	font-family:{% if settings.general_font != '' %}{{settings.general_font | replace: '+', ' '}}, {% endif %}{{ settings.general_font_backup }};
  	font-size: {{ settings.general_font_size | minus: 1 }}px;
	color: {{ settings.input_text_color }};
}

/***************************************************************************
 * 02. Structure
 */

html,body {
	min-height: 100%;
}
* {
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}
.container {
	margin:0;
	padding:0;
	/*overflow:hidden;*/
	display:block;
	width:100%;
	min-height:100%;
}
.content-container {
  	margin: auto;
 	max-width: 1100px;
}
#IE8bg {
 	display: none;
}

/**
 * Justify
 */

.just {
 	text-align: justify;
  	font-size: 0.01em;
}
	/* full-bleed text align justify needs a smaller font size */
	.product-grid.just {
  		font-size: 0.001px;
	}
.just > * {
  	display: inline-block;
	text-align: left;
  	vertical-align: top;
	font-size: {{settings.general_font_size}}px;
}
.just:after {
	content:'';
	display: inline-block;
	width: 100%;
  	height: 0;
}

/*****************************************************************************
 * 03. Header
 */
#Header {
	margin: 0;
  	padding: 0;
	position: relative;
	z-index: 100;
	vertical-align:baseline;
	text-align: center;
  	background: {{ settings.header_bg }};
  	color: {{ settings.header_text }};
}
#Header .wrap {
	position: relative;
  	line-height: 1;
  	z-index: 99;
}
#Header h1 {
	font-weight: {{settings.logo_font_style}};
	font-family:{% if settings.header_font != '' %}"{{settings.header_font | replace: '+', ' ' }}",{% endif %} {{ settings.header_font_backup }};
	font-size: {{ settings.logo_text_size }}px;
  	line-height: 1;
	color: {{settings.logo_color}};
	vertical-align:middle;
	margin: 0;
  	padding: 10px 0;
	display: block;
	position: relative;
	z-index: 3;
  	text-align: center;
  	clear: both;
}
#Header h1 a {
	text-decoration: none;
	color: {{settings.logo_color}};
	vertical-align:middle;
	display: table;
  	margin: auto;
  	padding: 0 20px;
}
#Header h1 a:hover {
	color: {{settings.logo_hover_color}};
}
#Header h1 a img {
	display: block;
  	width: {{ settings.logo_image_width }}px;
  	height: {{ settings.logo_image_height }}px;
  	max-width: 100%;
}
#Header nav {
	text-decoration: none;
  padding-top: 10px;
    text-transform: uppercase;
	position:relative;
	color: {{settings.secondary_text_color}};
	background-color: {{ settings.secondary_bg_color }};
	z-index: 3;
}
#Header nav a {
	padding: 12px 20px;
	text-decoration: none;
  	letter-spacing: 0.2em;
	display: inline-block;
	color: {{settings.secondary_link_color}};
	font-size: {{settings.secondary_font_size}}px;
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
	font-weight {{ settings.secondary_font_style }};
	-webkit-transition: color 0.2s ease-in;
	-moz-transition: color 0.2s ease-in;
	-o-transition: color 0.2s ease-in;
	-ms-transition: color 0.2s ease-in;
	transition: color 0.2s ease-in;
}
#Header nav a:hover,
#Header nav .active a {
	color: {{ settings.secondary_link_hover_color }};
}
#Header nav ul {
	/*-webkit-backface-visibility: hidden;
	-moz-backface-visibility: 	 hidden;
	-ms-backface-visibility: 	 hidden;
	-o-backface-visibility: 	 hidden;
	backface-visibility:	 	 hidden;*/
	position:relative;
	padding: 0;
	margin: 0;
	display:block;
}
{% if settings.site_show_borders %}
#Header nav ul {
	border-top: 1px solid {{ settings.site_border_color }};
	border-bottom: 1px solid {{ settings.site_border_color }};
}
{% endif %}
#Header nav ul li {
	display: inline-block;
	color: {{settings.secondary_text_color}};
	font-weight: {{settings.secondary_font_style}};
	font-size: {{settings.secondary_font_size}}px;
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
}
#Header nav ul li.mobile {
 	display: none;
}
#Header nav ul.submenu {
  	width: 100%;
    background: {{ settings.tertiary_bg_color }};
    color: {{ settings.tertiary_link_color }};
  	border: 0;
}
#Header nav ul.submenu a {
    color: {{ settings.tertiary_link_color }};
}
#Header nav ul.submenu a:hover,
#Header nav ul.submenu .active a {
    color: {{ settings.tertiary_link_hover_color }};
}
#Header nav a.mobile,
#Header #MainNav ul.submenu {
    display: none;
}

{% if settings.navigation_style == 'dropdown' %}
#Header nav ul li.has-sub-nav > a:after {
    font-family: FontAwesome;
 	content: ' \f107';
  	padding-left: 5px;
}

/* Dropdown */
#Header #MainNav ul.submenu {
  	display: none;
  	position: absolute;
  	top: 100%;
  	left: 0;
  	right: 0;
  	z-index: 999;
}
#Header nav ul li.back {
  	display: none;
}
{% endif %}

#TopBar {
	width: 100%;
	top: 0;
	left: 0;
	bottom: 0;
	text-align: right;
	padding: 20px 30px 0;
	z-index: 1;
    font-size: {{ settings.general_font_size | minus: 3 }}px;
      text-transform: uppercase;
  letter-spacing: .05em;
}
#TopBar #CartButton {
	margin: 0 0 0 15px;
}
#TopBar a,
#TopBar span.welcome {
	text-decoration: none;
	display: inline-block;
	margin: 0 0 10px 15px;
    color: {{ settings.header_link }};
}
#TopBar a.mobile {
 	display: none;
}
#TopBar a:hover,
#TopBar a:hover span,
#SearchToggle.open,
#SearchToggle.open .search-toggle {
	color: {{settings.header_link_hover}};
}
#TopBar #SearchToggle {
  	margin-left: 0;
 	float: left;
}

.search {
    position: relative;
  	height: auto;
    max-height: 0;
    width: 100%;
    z-index: 1000;
    background: {{ settings.input_bg_color }};
    overflow: hidden;
	-webkit-transition: max-height 0.2s ease-in-out;
	-moz-transition: max-height 0.2s ease-in-out;
	-o-transition: max-height 0.2s ease-in-out;
	-ms-transition: max-height 0.2s ease-in-out;
	transition: max-height 0.2s ease-in-out;
}
.search.open {
  {% unless settings.show_promo_banner %}border-top: 1px solid {{ settings.input_border_color }}; {% endunless %}
  	border-bottom: 1px solid {{ settings.input_border_color }};
    max-height: 100px;
}
.search-toggle {
	font-size: 11px;
	display: inline-block;
	margin: 0 0 0 5px;
	background: transparent;
	border: 0;
  	position: relative;
  	top: -1px;
  	right: 1px;
}
.search input:focus{
	outline: none;
}
.search .search-query {
	padding: 0 80px 0 30px;
	height: 100px;
	color: {{settings.input_text_color}};
	font-size: 30px;
	background: {{settings.input_bg_color}};
	outline:none;
	margin: 0;
	border: 0;
	width: 100%;
	font-family: {% if settings.general_font != '' %}"{{settings.general_font | replace: '+', ' '}}", {% endif %} {{ settings.general_font_backup }};
}
.search .remove-search {
  	position: absolute;
  	right: 30px;
  	top: 25px;
  	cursor: pointer;
	color: {{settings.input_text_color}};
  	width: 50px;
  	height: 50px;
}
.search .remove-search:before,
.search .remove-search:after {
    content: '';
    position: absolute;
    width: 50px;
    top: 50%;
  	left: 0;
    height: 1px;
    background: {{settings.input_text_color}};
    -webkit-transform: rotate(45deg);
    -moz-transform: rotate(45deg);
    transform: rotate(45deg);

	-webkit-transition: background 0.2s ease-in-out;
	-moz-transition: background 0.2s ease-in-out;
	-o-transition: background 0.2s ease-in-out;
	-ms-transition: background 0.2s ease-in-out;
	transition: background 0.2s ease-in-out;
}
.search .remove-search:after {
    -webkit-transform: rotate(-45deg);
    -moz-transform: rotate(-45deg);
    transform: rotate(-45deg);
}
.search .remove-search:hover:before,
.search .remove-search:hover:after {
 	background: {{settings.secondary_link_hover_color}};
}



/* Phone Number in header */
.info-box {
	float: left;
  	width: auto;
}

.info-box .header-phone {
    margin: 0;
    float: left;
    padding-right: 20px;
}

@media screen and (min-width: 641px) and (max-width: 992px) {
  .info-box .header-phone {
      clear: left;
  }
}

@media screen and (min-width: 300px) and (max-width: 640px) {
  .info-box {
  }
  
  .info-box .header-phone {
	width: 100%;
    text-align: center;
  }
  
  .mobile.toggle-menus {
	float: left !important;
  }
}


/* promo bar */

.promo {
  	text-align: center;
 	padding: 8px 30px 6px;
    font-size: {{ settings.promo_font_size }}px;
  	color: {{ settings.promo_text_color }};
    background: {{ settings.promo_bg_color }};
	border-bottom: 1px solid {{ settings.site_border_color }};
}
.promo strong {
  	display: inline-block;
  	margin-right: 5px;
    color: {{ settings.promo_title_color }};
}

/**
 * Content
 */
.content-wrapper {
	margin:0;
	padding:0 0 40px 0;
	min-width:100%;
	/*overflow:hidden;*/
	display:block;
	position:relative;
	text-align:center;
}
.template-product .content-wrapper,
.template-index .content-wrapper,
.template-collection .content-wrapper,
.template-list-collections .content-wrapper,
.template-search .content-wrapper
{
	padding:0;
}

/*****************************************************************************
/* 04. Index */

#Hero {
    position: relative;
    {% if settings.hero_type == 'image' %}
      {% if settings.index_hero_max != '' %}max-height: {{ settings.index_hero_max }}px;{% endif %}
    {% endif %}
    overflow: hidden;
  {% if settings.site_show_borders %}border-bottom: 1px solid {{ settings.site_border_color }};{% endif %}
}
#Hero img {
 	display: block;
  	margin: auto;
  	width: 100%;
}

/**
 * Video
 */

#HeroVideo iframe {
	position: absolute;
  	top: 0;
  	left: 0;
  	width: 100%;
  	height: 100%;
}

/**
 * Fader/slideshow
 */

.fader {
    position: relative;
	width: 100%;
	margin: 0 auto;
	-webkit-transform: translate3d(0, 0, 0);
    /*overflow: hidden; -- breaks fade transition */
}
.fader .slide {
    position: absolute;
    width: 100%;
    top: 0;
    z-index: 1;
    font-size: 0.001px;
}
.fader .slide > * {
  	width: 100%;
  	display: inline-block;
  	position: relative;
}
.fader .slide .hero-left,
.fader .slide .hero-right {
  	width: 50%;
}
.fader .slide img {
  	max-width: 100%;
}
/* loading */
.fader.loading {
	background:url({{ 'loading.gif' | asset_url }}) no-repeat center center;
  	min-height: 600px;
}
.fader.loading:after {
  content: 'loading';
  display: block;
  position: absolute;
  top: 50%;
  left: 0;
  right: 0;
  text-align: center;
  z-index: 9999;
  margin-top: 15px;
  font-style: italic;
  font-size: 12px;
  font-family: 'Georgia', times new roman, serif;
}
.fader.loading .slide {
 	opacity: 0;
}
/* end loading ****/

.hero .slide-pos {
   	position: absolute;
  	bottom: 0;
  	left: 0;
  	right: 0;
  	width: 100%;
  	text-align: center;
    -webkit-transition: all 0.8s ease-out;
    -moz-transition: all 0.8s ease-out;
    -ms-transition: all 0.8s ease-out;
    -o-transition: all 0.8s ease-out;
    transition: all 0.8s ease-out;

   	opacity: 0;
	filter: alpha(opacity=0);
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
}
.hero.hero-right .slide-pos {
  -webkit-transition-delay: 0.2s;
  transition-delay: 0.2s;
}
.active .hero .slide-pos {
    bottom: 68px;
    opacity: 1;
	filter: alpha(opacity=100);
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
}
.hero .slide-details {
  	z-index: 999;
  {% if settings.carousel_style == 'dark' %}
  	background: rgba(0,0,0,0.7);
  {% else %}
  	background: rgba(255,255,255,0.7);
  {% endif %}
  	display: inline-block;
  	margin: auto;
  	padding: 30px;
  	max-width: 85%;
  	min-width: 25%;
  	position: relative;
    -webkit-transition: top 0.15s ease-out;
    -moz-transition: top 0.15s ease-out;
    -ms-transition: top 0.15s ease-out;
    -o-transition: top 0.15s ease-out;
    transition: top 0.15s ease-out;
  	top: 0;
  	left: 0;
}
.hero-left .slide-details,
.hero-right .slide-details {
 	min-width: 50%;
}
{% unless settings.hero_type == 'image' and settings.index_hero_link == '' %}
.rgba .hero .slide-details:hover {
 	top: -5px;
}
{% endunless %}
.hero .slide-details h3 {
  	font-size: {{ settings.heading_font_size }}px;
  	margin: 0 0 0.5em;
  	text-transform: none;
}
.hero .slide-details p {
	font-family: {% if settings.secondary_font != '' %}"{{settings.secondary_font | replace: '+', ' ' }}",{% endif %} {{ settings.secondary_font_backup }};
  	font-size: {{ settings.general_font_size }}px;
  	margin: 0;
}
.hero .slide-details h3,
.hero .slide-details p {
  {% if settings.carousel_style == 'dark' %}
 	color: #ffffff;
  {% else %}
  	color: #555555;
  {% endif %}
}
.bx-prev,
.bx-next{
	position: absolute;
	height: 80px;
	line-height: 80px;
	width: 100px;
	font-size: 80px;
	text-align: center;
	color: #fff;
	top: 50%;
	left: 0;
	z-index: 99;
	margin-top: -40px;
	cursor: pointer;
	opacity:0;
	-webkit-transition: opacity 0.2s ease-in-out;
	-moz-transition: opacity 0.2s ease-in-out;
	-o-transition: opacity 0.2s ease-in-out;
	-ms-transition: opacity 0.2s ease-in-out;
	transition: opacity 0.2s ease-in-out;
}
#Hero:hover .bx-prev,
#Hero:hover .bx-next {
	opacity:0.7;
}
#Hero:hover .bx-prev:hover,
#Hero:hover .bx-next:hover{
	opacity:1;
  	color: #fff;
}
.bx-next{
	left: auto;
	right: 0;
}
.bx-pager div:hover,
.bx-pager div.active{
	filter: alpha(opacity=100);
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=100)";
	opacity:1;
}

.bx-pager {
	position: absolute;
	width: 100%;
	height: 5px;
	bottom: 0px;
	text-align: center;
	z-index: 99;
	padding: 0;
  	margin: 0;
}
.bx-pager > div {
	display: block;
	width: 50%;
	height: 5px;
	margin: 0;
